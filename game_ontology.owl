          <owl:onProperty rdf:resource="#is_professional"/>
          <owl:someValuesFrom rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:someValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

