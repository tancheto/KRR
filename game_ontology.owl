        <rdf:Description rdf:about="#Paper"/>
        <rdf:Description rdf:about="#Pencil"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
  <rdfs:subPropertyOf rdf:resource="#require"/>
</owl:ObjectProperty>

